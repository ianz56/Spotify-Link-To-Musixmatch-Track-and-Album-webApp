# Musixmatch Configuration
# Note: MXM_API should be a valid usertoken now, not a regular API key
MXM_API=your_musixmatch_usertoken_here
MXM_API2=your_second_musixmatch_usertoken_here

# Spotify Configuration
SPOTIPY_CLIENT_ID=your_spotify_client_id_here
SPOTIPY_CLIENT_SECRET=your_spotify_client_secret_here

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWD=your_redis_password_here

# Flask Application Secret
secret_key=your_random_secret_string_here

FLASK_ENV=development
FLASK_DEBUG=1
